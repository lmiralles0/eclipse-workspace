#include <stdio.h>
#include <stdlib.h>
#include <string.h>



int inicializaArrays(int* pArrays, int longitud, int valorInicial)//esta funcion devuelve -1 como error y 0 como todo ok
{
	int retorno = -1;
	int i;

	if(pArrays != NULL && longitud > 0)
	{
		retorno = 0;
		for(i= 0; i < longitud; i++)
		{
			pArrays[i] = valorInicial;

		}
	}
	return retorno;
}


